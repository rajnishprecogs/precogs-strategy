{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/docs/$1"
        }
    ],
    "functions": {
        "api/*.js": {
            "runtime": "@vercel/node@3"
        }
    }
}